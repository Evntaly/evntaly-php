{
    "name": "evntaly/evntaly-php",
    "description": "PHP SDK for the Evntaly event tracking platform",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Evntaly\\": "src/Evntaly/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Evntaly\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Evntaly Team",
            "email": "support@evntaly.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.0",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-openssl": "*",
        "guzzlehttp/guzzle": "^7.5"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.14",
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^9.6 || ^10.0",
        "squizlabs/php_codesniffer": "^3.12"
    },
    "suggest": {
        "justinrainbow/json-schema": "For JSON Schema validation of events (^5.2)",
        "ratchet/pawl": "^0.4.1 - For WebSocket client functionality",
        "react/async": "^4.0 - For asynchronous event processing",
        "react/promise": "^3.0 - For promise-based operations",
        "react/event-loop": "^1.3 - For event scheduling",
        "league/csv": "^9.8 - For CSV export/import functionality",
        "symfony/framework-bundle": "^5.4|^6.0 - For Symfony integration",
        "laravel/framework": "^8.0|^9.0|^10.0 - For Laravel integration"
    },
    "keywords": [
        "evntaly",
        "analytics",
        "events",
        "tracking",
        "user identification",
        "batch processing",
        "php sdk",
        "context awareness",
        "sampling",
        "performance tracking",
        "webhooks",
        "realtime",
        "export",
        "import"
    ],
    "config": {
        "sort-packages": true
    }
}
